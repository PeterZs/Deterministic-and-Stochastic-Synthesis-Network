    new_height: 144
    new_width: 144
    is_color: true
    shuffle: true
    root_folder: ""
  }
  random_crop_param {
    lower_bound: 416
    upper_bound: 417 
  }
  bilinear_interp_param {
    scale: 0.3333333
  }
} 
